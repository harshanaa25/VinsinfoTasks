import java.util.LinkedHashSet;
public class _13_Remove_Duplicates {
    public static void main(String[] args) {
        int[] arr = {1, 2, 2, 3, 3, 4};
        LinkedHashSet<Integer> set = new LinkedHashSet<>();
        for (int num : arr) {
            set.add(num);
        }
        System.out.println(set);
    }
}
