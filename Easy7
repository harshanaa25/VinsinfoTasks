public class_7_Prime_Check {
    public static void main(String[] args) {
        int n = 11;
        boolean prime = true;
        if (n <= 1) prime = false;
        for (int i = 2; i <= n / 2; i++) {
            if (n % i == 0) {
                prime = false;
                break;
            }
        }
        System.out.println(prime);
    }
}
