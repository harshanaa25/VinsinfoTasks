public class _15_List_Contains {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4};
        int target = 3;
        boolean found = false;
        for (int num : arr) {
            if (num == target) {
                found = true;
                break;
            }
        }
        System.out.println(found);
    }
}
